<?xml version="1.0" encoding="utf-8"?>

<rotate xmlns:android="http://schemas.android.com/apk/res/android" android:fromDegrees="45">
    <layer-list>
        <item android:right="1.5dp" android:bottom="1.5dp">
            <shape  android:shape="rectangle" >
                <solid android:color="#5000" />

                <size android:width="10dp" android:height="10dp"/>
                <!--        <padding android:bottom="6dp" android:left="10dp" android:right="10dp" android:top="6dp" />-->
                <stroke android:width="4dp" android:color="#0000"/>
            </shape>
        </item>
        <item>
            <shape  android:shape="rectangle" >
                <solid android:color="#4C4C4C" />

                <size android:width="10dp" android:height="10dp"/>
                <!--        <padding android:bottom="6dp" android:left="10dp" android:right="10dp" android:top="6dp" />-->
                <stroke android:width="7dp" android:color="#0000"/>
            </shape>
        </item>
    </layer-list>
</rotate>
