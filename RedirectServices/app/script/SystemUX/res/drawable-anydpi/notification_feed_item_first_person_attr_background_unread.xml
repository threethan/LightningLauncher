<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="?ocPanelBackground" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="?ocCardBackground" />
        </shape>
    </item>
    <item>
        <selector>
            <item android:state_hovered="true">
                <shape android:shape="oval">
                    <solid android:color="?ocHoverOverlay" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>