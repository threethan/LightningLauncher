<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:height="@dimen/abc_star_small" android:width="@dimen/abc_star_small">
        <drawable class="com.oculus.ocui.drawable.OCInteractionOverlayDrawable">
            <selector>
                <item android:state_checked="true">
                    <shape android:shape="oval">
                        <solid android:color="?ocControlBackgroundActive" />
                    </shape>
                </item>
                <item>
                    <shape android:shape="oval">
                        <solid android:color="?ocControlBackgroundInactive" />
                    </shape>
                </item>
            </selector>
        </drawable>
    </item>
    <item android:gravity="center">
        <selector>
            <item android:state_checked="true">
                <shape android:shape="oval">
                    <size android:height="@dimen/abc_button_padding_horizontal_material" android:width="@dimen/abc_button_padding_horizontal_material" />
                    <solid android:color="?ocControlIcon" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>